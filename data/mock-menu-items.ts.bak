export const getMenuItemsByCategory = (items: MenuItem[], category: string): MenuItem[] => {
  if (category === "all") {
    return items
  }

  const categoryMap: Record<string, string[]> = {
    breakfast: ["South Indian", "Breakfast"],
    lunch: ["Main", "Bowl"],
    snacks: ["Snack", "Street Food", "Chaat"],
    beverages: ["Drink"],
    desserts: ["Indian Sweet", "Frozen", "Bakery", "Fusion", "Dessert"],
  }

  const mappedCategories = categoryMap[category] || [category]
  return items.filter((item) => mappedCategories.includes(item.category))
}

export const getAvailableMenuItems = (): MenuItem[] => {
  return mockMenuItems.filter((item) => item.available)
}

export const getPopularMenuItems = (): MenuItem[] => {
  return mockMenuItems.filter((item) => item.isPopular)
}

export const getBestsellerMenuItems = (): MenuItem[] => {
  return mockMenuItems.filter((item) => item.isBestseller)
}

export const getMenuItemById = (id: number): MenuItem | undefined => {
  return mockMenuItems.find((item) => item.id === id)
}

export const getMenuItemsByTruck = (truckId: number): MenuItem[] => {
  return mockMenuItems.filter((item) => item.truckId === truckId)
}
